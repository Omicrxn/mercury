{
	"name": "@svecodocs/docs",
	"version": "0.0.0",
	"type": "module",
	"scripts": {
		"dev": "pnpm \"/dev:/\"",
		"dev:content": "velite dev --watch",
		"dev:svelte": "vite dev",
		"build": "velite && node ./scripts/update-velite-output.js && pnpm build:search && vite build",
		"build:search": "node ./scripts/build-search-data.js",
		"preview": "vite preview",
		"check": "velite && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "pnpm build:content && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
	},
	"devDependencies": {
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.525.0",
		"@svecodocs/kit": "latest",
		"@sveltejs/adapter-cloudflare": "^7.0.4",
		"@sveltejs/kit": "^2.22.2",
		"@sveltejs/vite-plugin-svelte": "^5.1.0",
		"@tailwindcss/vite": "^4.1.11",
		"@tanstack/table-core": "^8.21.3",
		"bits-ui": "^2.8.10",
		"clsx": "^2.1.1",
		"embla-carousel-svelte": "^8.6.0",
		"formsnap": "^2.0.1",
		"layerchart": "^2.0.0-next.18",
		"mdsx": "^0.0.7",
		"mode-watcher": "^1.1.0",
		"paneforge": "1.0.0",
		"phosphor-svelte": "^3.0.1",
		"shiki": "^3.7.0",
		"svelte": "^5.34.9",
		"svelte-check": "^4.2.2",
		"svelte-sonner": "^1.0.5",
		"sveltekit-superforms": "^2.27.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.11",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.8.3",
		"vaul-svelte": "1.0.0-next.7",
		"velite": "^0.2.4",
		"vite": "^7.0.0",
		"wrangler": "^4.22.0"
	},
	"dependencies": {
		"@omicrxn/mercury": "^0.5.2",
		"motion": "^12.19.3",
		"runed": "^0.29.1"
	}
